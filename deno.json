{
  "compilerOptions": {
    "allowJs": true,
    "strict": false,
    "target": "esnext",
    "esModuleInterop": true,
    "module": "esnext",
    "jsx": "react",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "fmt": {
    "options": {
      "indentWidth": 2,
      "lineWidth": 120,
      "singleQuote": true,
      "useTabs": false
    }
  }
}
